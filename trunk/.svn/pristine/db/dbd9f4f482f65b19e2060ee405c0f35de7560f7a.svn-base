public interface ProductInfoDao extends JpaRepository<TProductInfoEntity,Long> {
    //查询商品表中的所属一级分类的个数（用于是否删除商品分类）
     Long countTProductInfoEntitiesByFCategoryIdOneEquals(Long id);
